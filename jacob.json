{
  "language": "TypeScript",
  "installCommand": "npm install",
  "formatCommand": "npx eslint --fix .",
  "buildCommand": "npm run build",
  "style": "CSS",
  "iconSet": "Material UI",
  "componentExamples": "/src/components/ExampleComponent.js",
  "apiEndpointsExamples": "/src/api/exampleEndpoint.js",
  "pageExamples": "/src/pages/ExamplePage.js",
  "directories": {
    "components": "/src/components",
    "pages": "/src/pages",
    "types": "/src/types",
    "styles": "/src/styles",
    "staticAssets": "/public/assets"
  },
  "stateManagement": {
    "tool": "None"
  },
  "testing": {
    "writeTests": true,
    "testFramework": "Jest"
  },
  "storybook": {
    "writeStories": true,
    "storiesLocation": "/src/stories"
  },
  "envVariables": {
    "exampleFile": "/.env.example"
  },
  "env": ""
}